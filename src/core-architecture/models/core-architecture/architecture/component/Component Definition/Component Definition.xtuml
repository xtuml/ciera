INSERT INTO O_TFR
	VALUES ("a7a72b98-7905-4169-9e1d-82d499328f4b",
	"23f9d7b1-3127-4414-9299-e535bda771bf",
	'render',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'utility_initializers = "";
separator = "";
select many util_refs related by self->UtilityInComponent[R4558];
for each util_ref in util_refs
  util_ref.render();
  utility_initializers = utility_initializers + separator + "this." + util_ref.utility_name + " = new " + util_ref.utility_name + "(this);";
  separator = "\n";
end for;
utilities = T::body();

select many comp_ports related by self->Port[R417];
for each port in comp_ports
  port.render_acccessor_decl();
end for;
ports = T::body();

for each port in comp_ports
  port.render_acccessor_init();
end for;
port_initializers = T::body();

select many funcs related by self->Function[R405];
for each function in funcs
  function.render();
end for;
functions = T::body();

select many objs related by self->Class[R408];
for each obj in objs
  select many class_ops related by obj->Operation[R416] where (selected.is_class_based);
  for each operation in class_ops
    operation.render();
  end for;
end for;
class_operations = T::body();

select many relationships related by self->Relationship[R448];
for each rel in relationships
  rel.render_modifiers();
end for;
relationship_modifiers = T::body();

for each port in comp_ports
  port.render_acccessor();
end for;
port_accessors = T::body();

init = "";
sep = "";
select many init_functions related by self->Function[R4561];
for each init_function in init_functions
  init = init + sep + init_function.name + "();";
  sep = "\n";
end for;

select many csms related by self->Class[R408]->StateMachine[R4750] where not selected.is_instance and SORT::ascending(attr:"class_name");
for each csm in csms
  csm.render_handler();
end for;
csm_handlers = T::body();

for each csm in csms
  csm.render_persist();
end for;
csm_persists = T::body();

for each csm in csms
  csm.render_load();
end for;
csm_loads = T::body();

if not_empty csms
  ::addStringImport(package:"io.ciera.runtime.api.application", class_name:"Event");
  ::addStringImport(package:"io.ciera.runtime.api.exceptions", class_name:"EventTargetException");
  ::addStringImport(package:"java.io", class_name:"IOException");
  ::addStringImport(package:"java.io", class_name:"ObjectInputStream");
  ::addStringImport(package:"java.io", class_name:"ObjectOutputStream");
end if;

::addStringImport(package:"io.ciera.runtime.api.domain", class_name:"PersistentDomain");
::addStringImport(package:"io.ciera.runtime.domain", class_name:"AbstractDomain");

select one file related by self->File[R401];
imports = file.get_imports();

T::include(file:"component/t.componentdefinition.java");
T::emit(file:file.path + "/" + self.name + file.extension);
T::clear();',
	1,
	'',
	"00000000-0000-0000-0000-000000000000",
	0,
	0);

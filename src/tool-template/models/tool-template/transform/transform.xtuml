INSERT INTO S_SYNC
	VALUES ("02c391f4-64f7-49c8-98ae-d6d473a3b457",
	"00000000-0000-0000-0000-000000000000",
	'transformRSLEmptyCheckOperation',
	'',
	'emp_op = param.emp_op;
smt = param.smt;
expr_number = param.expr_number;

// get the operand
select one term related by emp_op->Y_EmptyCheck[R3091]->Y_Term[R3067];

// create unary operation
create object instance expr of Expression;
relate expr to smt across R775;
expr.expression_number = expr_number;
boolean_type = ::TypeReference_getBuiltinTypeReference(name:"boolean");
relate boolean_type to expr across R795;
create object instance unary of UnaryOperation;
relate unary to expr across R776;

// translate the operator
if ("" != emp_op.EMPTY)
  unary.operator = "isEmpty";
  unary.invocation = true;

  // transform the operand expression
  ::transformRSLTerm(term:term, smt:smt, expr_number:expr_number + ".1");
  select any op related by smt->Expression[R775] where (selected.expression_number == expr_number + ".1");
  relate op to unary across R777;
elif ("" != emp_op.NOT_EMPTY)
  unary.operator = "!";
  unary.invocation = false;

  // create additional ''empty'' expression
  create object instance expr2 of Expression;
  relate expr2 to smt across R775;
  expr2.expression_number = expr.expression_number + ".1";
  relate expr2 to unary across R777;
  boolean_type = ::TypeReference_getBuiltinTypeReference(name:"boolean");
  relate boolean_type to expr2 across R795;
  create object instance unary2 of UnaryOperation;
  relate unary2 to expr2 across R776;
  unary2.operator = "isEmpty";
  unary2.invocation = true;

  // transform the operand expression
  ::transformRSLTerm(term:term, smt:smt, expr_number:expr2.expression_number + ".1");
  select any op related by smt->Expression[R775] where (selected.expression_number == expr2.expression_number + ".1");
  relate op to unary2 across R777;
end if;',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'',
	0,
	0);

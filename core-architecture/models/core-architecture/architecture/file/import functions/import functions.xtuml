INSERT INTO S_SYNC
	VALUES ("5f38af04-404d-4723-a184-3db026951f01",
	"00000000-0000-0000-0000-000000000000",
	'addStringImport',
	'',
	'// select all rendering files
// exclude files which are defined in the same pacakge as the import
select many files from instances of File where selected.package != param.package and selected.rendering;
if not_empty files
  
  // select or create an import
  select any type_import from instances of TypeImport where selected.package == param.package and selected.class_name == param.class_name;
  if empty type_import
    create object instance type_import of TypeImport;
    type_import.package = param.package;
    type_import.class_name = param.class_name;
  end if;
  
  // apply the type import to each file in active render
  for each file in files
    select any existing_import_ref related by file->TypeImportReference[R402] where selected.import_package == type_import.package and selected.import_class_name == type_import.class_name;
    if empty existing_import_ref
      create object instance import_ref of TypeImportReference;
      relate file to type_import across R402 using import_ref;
    end if;
  end for;

end if;',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'',
	0,
	1);
